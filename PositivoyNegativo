#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;

void main()
{
	int A;
	cout <<"Ingrese un numero: ";
	cin >> A;
	if(A < 0)
	   cout<<"El numero es negativo";
	else
		cout<<"El numero es positivo";
	
	getch();

}
