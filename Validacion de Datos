void main()
{int e;
 do
 {cin>> e;
  cout<<"Ingrese su edad: "<<e;
 }while(0>=e && e<=100);
  if(e >=18) 
  {cout<<"Usted esta habil para votar";
  }
  else
  {cout<<"Usted NO esta habil para votar";
  }
