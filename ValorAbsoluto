// Valor Absoluto bpr.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;

void main()
{
	int A;
	cout <<"Ingrese un numero: ";
	cin >> A;
	if(A < 0)
	   A = -A;

	cout <<"El valor absoluto es:"<< A;
	getch();

}
