#include "stdafx.h"
#include "conio.h"
#include <iostream>
using namespace std;

int main()
{ float h, j, k, t;
cout<< "Ingrese 1 si el personal es ingeniero o 0 si es tecnico";
cin>> t;
if (t==1)
{
cout<< "Ingrese el numero de horas trabajadas";
cin>> h;
j=25*h;
cout<< "El total del jornal del ingeniero es:"<<j;
}
else
{
cout<< "Ingrese el numero de horas trabajadas";
cin>> h;
k=5*h;
cout<< "El total del jornal del tecnico es:"<<k;
}
getch();
return 0;
}
